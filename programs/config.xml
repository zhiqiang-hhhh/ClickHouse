<clickhouse>
    <logger>
        <level>trace</level>
        <console>true</console>
    </logger>

    <listen_host>0.0.0.0</listen_host>
    <http_port>8777</http_port>
    <tcp_port>9888</tcp_port>
    <mysql_port>9999</mysql_port>

    <path>./</path>

    <mlock_executable>true</mlock_executable>

    <users>
        <default>
            <password/>

            <networks>
                <ip>::/0</ip>
            </networks>

            <profile>default</profile>
            <quota>default</quota>
            <access_management>1</access_management>
        </default>
    </users>

    <profiles>
        <default/>
    </profiles>

    <quotas>
        <default/>
    </quotas>
</clickhouse>

